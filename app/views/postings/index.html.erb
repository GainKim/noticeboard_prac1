<h1>Listing All Postings</h1>

<form href="/postings/create" method="POST">
 <div class="form-group">
    <label for="title">Title</label>
    <input type="title" class="form-control" name="title" id="title">
  </div>
  <div class="form-group">
    <label for="body">Body</label>
    <textarea class="form-control" name="body" rows="5" id="comment"></textarea>
  </div>
  <button type="submit" class="btn btn-primary">Submit</button>
</form>

    <% @all_postings.each do |p| %>
        <h2>Title: <%= p.title %></h2>
        <p><%= p.body %></p>
    <% end %>
